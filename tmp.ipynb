    "import numpy as np\n",
    "import pandas as pd\n",
    "from statsmodels.api import OLS\n",
    "\n",
    "n = 1000\n",
    "p = 100\n",
    "\n",
    "y = np.random.normal(size = n)\n",
    "X = np.random.normal(size = n * p).reshape(-1, p)\n",
    "\n",
    "fit = OLS(y, X).fit()\n",
    "np.sum(fit.pvalues < 0.05)\n"
   ]
  }
 ],
